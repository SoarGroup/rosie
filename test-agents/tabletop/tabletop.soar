sp {top-state*perception-monitor*predicate-monitor*elaborate*default*predicates
   (state <s> ^superstate nil
              ^world.predicates <preds>)
-->
   (<preds> ^predicate <inp>)
   (<inp> ^handle in1)

   (<preds> ^predicate <onp>)
   (<onp> ^handle on1)
}

