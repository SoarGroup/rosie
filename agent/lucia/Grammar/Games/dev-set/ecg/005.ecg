// Conditional commands
construction IfConditionThenStatement
   subcase of Conditional
   constructional
      constituents
         conditionPart: IfConditionThen
         statement: Declarative
   meaning: IfThenStatement
      constraints
         self.m.condition <--> conditionPart.condition.declarative.m
         self.m.statement <--> statement.m


schema IfThenStatement
   roles
      condition
      statement
