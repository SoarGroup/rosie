// Spatial shapes
schema Hall
   subcase of SpatialShape
   constraints
      rosie-category <-- spatial-shape
      spatial-shape <-- hall1

schema End
   subcase of SpatialShape
   constraints
      rosie-category <-- spatial-shape
      spatial-shape <-- end1

schema Intersection
   subcase of SpatialShape
   roles
   constraints
      rosie-category <-- spatial-shape
      spatial-shape <-- intersection

// Objects
schema Stapler
   subcase of RosieObject
   roles
      name
   constraints
      rosie-category <-- object
      movable <-- true
      name <-- stapler

schema Soda
   subcase of Block
   constraints
      shape <-- soda1

schema Trash
   subcase of Block
   constraints
      shape <-- trash1

//  Concepts
schema Concept
   subcase of Entity
   roles
      concept-type
      concept-handle

schema Task
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- task1

schema Units
   subcase of Concept
   roles
      type
   constraints
      rosie-category <-- concept

schema Goal
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- goal
