//  Pronouns
construction ME
   subcase of Pronoun
   constructional
     constraints
       self.features.case <-- "nom"
       self.features.person <-- "1st"
   form
      constraints
         self.f.orth <-- "me"
   meaning
      constraints
         self.m.category <-- @animate

//  Posessive pronouns

//	Specifiers
construction THESE
   subcase of NPSpecifier
   form
      constraints
         self.f.orth <-- "these"

// Quantifiers
construction ALL
   subcase of Quantifier
   form
      constraints
         self.f.orth <-- "all"
   meaning: Quantification
      constraints
         self.m.type <-- "all"

construction ALL-OF
   subcase of Quantifier
   form
      constraints
         self.f.orth <-- "all of"
   meaning: Quantification
      constraints
         self.m.type <-- "all"

construction SOME
   subcase of Quantifier
   form
      constraints
         self.f.orth <-- "some"
   meaning: Quantification
      constraints
         self.m.type <-- "some"

//  Cardinal numbers

//	Conjunctions
construction AS
   subcase of Conjunction
   form
      constraints
         self.f.orth <-- "as"

