// Spatial shapes
schema Hall
   subcase of SpatialShape
   constraints
      rosie-category <-- spatial-shape
      spatial-shape <-- hall1

schema End
   subcase of SpatialShape
   constraints
      rosie-category <-- spatial-shape
      spatial-shape <-- end1

schema Intersection
   subcase of SpatialShape
   roles
   constraints
      rosie-category <-- spatial-shape
      spatial-shape <-- intersection

// Objects
schema Stapler
   subcase of RosieObject
   roles
      name
   constraints
      rosie-category <-- object
      movable <-- true
      name <-- stapler

schema Soda
   // PL 11/3/2020
   // This schema is being changed to fit the new format
   // used with objects in the Robot domain.
   // The old form used with the 209 baseline only
   // produced a new object, it was never grounded
   // in the world.
   // This change makes Robot 004 work.
   subcase of ObjectNotBlock  // Changed from Block
   constraints
      category <-- soda1  // Changed from shape <-- soda1

schema Trash
   subcase of Block
   constraints
      shape <-- trash1

//  Concepts
schema Concept
   subcase of Entity, PropertyDescriptor
   roles
      concept-type
      concept-handle

schema Task
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- task1

schema Units
   subcase of Concept
   roles
      type
   constraints
      rosie-category <-- concept

schema Goal
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- goal
