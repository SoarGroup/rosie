// Objects
// This schema is for RosieObjects whose category
// is object and not block.
schema ObjectNotBlock  //  For 001
   subcase of RosieObject
   roles
      shape
      color
      size
	  category
   constraints
      rosie-category <-- object
      movable <-- true

schema Fork  //  For 001
   subcase of ObjectNotBlock
   constraints
      movable <-- true
      category <-- fork1

schema Drawer  //  For 003
   subcase of ObjectNotBlock
   constraints
      movable <-- true
      category <-- drawer1

schema Mug  //  For 004
   subcase of ObjectNotBlock
   constraints
      movable <-- true
      category <-- mug1

schema Fridge  //  For 004
   subcase of ObjectNotBlock
   constraints
      movable <-- false
      category <-- fridge1
