   meaning: Fridge

construction DEGREES  //  For 014
   subcase of UnitsName
   form
      constraints
         self.f.orth <-- "degrees"
   meaning
      constraints
         self.m.type <-- "angle"
         self.m.concept-handle <-- "degrees"

