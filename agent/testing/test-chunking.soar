sp {rosie*testing*propose*test-chunking
  (state <s> ^name rosie)
-->
  (<s> ^operator <o> + > !)
  (<o> ^name test-chunking)
}

sp {test-chunking*elaborate*problem-space
  (state <s> ^name test-chunking)
-->
  (<s> ^problem-space <ps>)
  (<ps> ^name action
        ^execution-type external
         ^learning <learn>)
}

sp {test-chunking*elaborate*world
  (state <s> ^name test-chunking)
-->
  (<s> ^world <w>)
  (<w> ^robot <robot>
        ^objects <objs>
        ^predicates <preds>)
  (<robot> ^moving-status stopped
            ^current-location <loc>)

  (<objs> ^object <obj1> <obj2> <loc>)
  (<obj1> ^handle 1
           ^item-type object
           ^predicates <preds1>)
  (<preds1> ^color red ^shape box ^visible true ^category object)

  (<obj2> ^handle 2
           ^item-type object
           ^predicates <preds2>)
  (<preds2> ^color blue ^shape box ^visible true ^category object)

  (<loc> ^handle 3
          ^item-type object
          ^predicates <loc-preds>)
  (<loc-preds> ^name kitchen ^visible true ^category location)

  (<preds> ^predicate <in-pred>)
  (<in-pred> ^handle in1
              ^instance <i1> <i2>)
  (<i1> ^1 <obj1> ^2 <loc>)
  (<i2> ^1 <obj2> ^2 <loc>)
}

sp {test-chunking*elaborate*action-id
  (state <s> ^name test-chunking)
-->
  (<s> ^action-id <aid>)
  (<aid> ^handle deliver1 ^item-type action ^procedural <proc> ^goal <goal>)
  (<proc> ^op_name op_deliver1
           ^arg1 <arg1>
           ^arg2 <arg2>)
  (<arg1> ^arg-type object ^importance required ^arg-status real ^id <obj-slot1>)
  (<arg2> ^arg-type predicate ^importance required ^arg-status real ^id <pred-slot2> ^2 <obj-slot2>)
  (<goal> ^predicate <pred>)
  (<pred> ^type relation
           ^id <pred-slot2>
           ^1 <obj-slot1>
           ^2 <obj-slot2>)
}

sp {test-chunking*elaborate*action-operator
  (state <s> ^name test-chunking
              ^world.objects <objs>)
  (<objs> ^object <obj1> <loc>)
  (<obj1> ^handle 1)
  (<loc> ^handle 3)
-->
  (<s> ^action-operator <aop>)
  (<aop> ^name op_deliver1
          ^action-handle deliver1
          ^arg1 <arg1>
          ^arg2 <arg2>)
  (<arg1> ^arg-type object
           ^id <obj1>)
  (<arg2> ^arg-type predicate
           ^handle in1
           ^2 <loc>)
}

