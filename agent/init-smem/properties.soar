## Cardinal Dirs ## 
# +@cardinal-dirs
smem --add {
   (@north1 ^handle north1 ^property @cardinal-direction1 ^yaw 1.507 ^item-type concept)
   (@east1 ^handle east1 ^property @cardinal-direction1 ^yaw 0.0 ^item-type concept)
   (@south1 ^handle south1 ^property @cardinal-direction1 ^yaw -1.507 ^item-type concept)
   (@west1 ^handle west1 ^property @cardinal-direction1 ^yaw 3.14 ^item-type concept)
}

## Units  ##
smem --add {
   (@meters ^handle meters ^property @unit ^item-type concept)
   (@feet ^handle feet ^property @unit ^item-type concept)
   (@inches ^handle inches ^property @unit ^item-type concept)
   (@centimeters ^handle centimeters ^property @unit ^item-type concept)
   (@minutes ^handle minutes ^property @unit ^item-type concept)
   (@seconds ^handle seconds ^property @unit ^item-type concept)
   (@hours ^handle hours ^property @unit ^item-type concept)
   (@degrees ^handle degrees ^property @unit ^item-type concept)
   (@times ^handle times ^property @unit ^item-type concept)
}

## Relative Directions ##
smem --add {
   (@right1 ^handle right1 ^property @relative-direction1 ^side -1 ^item-type concept ^yaw -1.57)
   (@left1 ^handle left1 ^property @relative-direction1 ^side 1 ^item-type concept ^yaw 1.57)
   (@around1 ^handle around1 ^property @relative-direction1 ^side -1 ^item-type concept ^yaw 3.14)
   (@forward1 ^handle forward1 ^property @relative-direction1 ^item-type concept)
   (@up1 ^handle up1 ^property @relative-direction1 ^item-type concept)
   (@down1 ^handle down1 ^property @relative-direction1 ^item-type concept)
}

