#   (@on2 ^handle on2 ^property @activation1 ^item-type concept)
#   (@off2 ^handle off2 ^property @activation1 ^item-type concept)

   (@meat1 ^handle meat1 ^type state ^item-type property)
   (@cooked1 ^handle cooked1 ^property @meat1 ^item-type concept)
   (@raw1 ^handle raw1 ^property @meat1 ^item-type concept)

# AM: Changed to is-open1: open2 not-open1 in concepts.soar
#   (@door2 ^handle door2 ^type state ^item-type property)
#   (@open2 ^handle open2 ^property @door2 ^item-type concept)
#   (@closed2 ^handle closed2 ^property @door2 ^item-type concept)

   (@action1 ^handle action1 ^type state ^item-type property)
   (@last-moved1 ^handle last-moved1 ^property @action1 ^item-type predicate)
}

