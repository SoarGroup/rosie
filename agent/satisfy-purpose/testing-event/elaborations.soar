sp {testing-event*elaborate*purpose
   (state <s> ^name testing-event
              ^superstate.purpose <purp>)
-->
   (<s> ^purpose <purp>)
}

sp {testing-event*elaborate*command-name
   (state <s> ^name testing-event
              ^purpose.parameters.command.spelling <cmd-name>)
-->
   (<s> ^command-name <cmd-name>)
}
