### FAILED REFERENT
sp {propose*process-semantics*failed-referent
 #:interrupt
   (state <s> ^name comprehension
              ^assigner <ass>)
   (<ass> ^merged-receiver <mre>
         -^processed-semantics <sg>
          ^lt.semantic-structure { <> *copy* <> *embed* <ss> }) 
  (<mre> ^lt.relation <rel>
        -^referent
         ^head <head>)
  (<head> ^constraint <con>
          ^failed-grounding <fg>)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name process-semantics
         ^assigner <ass>
         ^attribute <ss>
         ^relation <val>)
   (<val> ^2 <con> 
          ^handle <rel>)}

