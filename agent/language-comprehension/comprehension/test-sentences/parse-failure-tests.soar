          ^current-word <first>
          ^original-sentence <first>)
   (<first> ^spelling |*|
            ^next <w0>
            ^complete-sentence |This is a tasty triangle.|)
   (<w0> ^spelling |this|
         ^next <w1>)
   (<w1> ^spelling |is|
         ^next <w5>)
   (<w5> ^spelling |a|
         ^next <w6>)
   (<w6> ^spelling |tasty|
         ^next <w7>)
   (<w7> ^spelling |triangle|
         ^next <w9>)
   (<w9> ^spelling |.|
         ^next nil)
}

