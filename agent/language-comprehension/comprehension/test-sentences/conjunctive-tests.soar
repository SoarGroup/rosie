learn --off
## This is for logging
sp {elaborate*state*current-sentence-number
   (state <s> ^name comprehension
              ^superstate <ss>
              ^segment.original-sentence <first>)
   (<ss> ^current-sentence-number <csn>)
-->
   (<first> ^current-sentence-number <csn>)}

sp {apply*create-scripted-sentences*sentence-number
   (state <s> ^name rosie
              ^operator.name create-scripted-sentences)
-->
   (<s> ^current-sentence-number 1
        ^max-sentence-number 12  
        ^failure-count 0
        ^success-count 0)}

sp {elaborate*state*sentence*This-is-red-and-small
   (state <s> ^name comprehension
              ^superstate <ss>
              ^segment <seg>)
   (<ss> ^current-sentence-number 1)
-->
   (<seg> ^input-sentence <first>
          ^current-word <first>
          ^original-sentence <first>)
   (<first> ^spelling |*|
            ^next <w0>
            ^complete-sentence |This is red and small.|)
   (<w0> ^spelling |this|
         ^next <w1>)
   (<w1> ^spelling |is|
         ^next <w2>)
   (<w2> ^spelling |red|
         ^next <w3>)
   (<w3> ^spelling |and|
         ^next <w4>)
   (<w4> ^spelling |small|
         ^next <w5> )
   (<w5> ^spelling |.|
         ^next nil)
}

