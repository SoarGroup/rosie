           (<north>  ^spelling |north| 
                     ^structure-type CARDINAL
                     ^number singular
                     ^referent @north1)

           (<south>  ^spelling |south| 
                    ^structure-type CARDINAL
                    ^number singular
                    ^referent @south1)

          (<east>  ^spelling |east| 
                    ^structure-type CARDINAL
                    ^number singular
                    ^referent  @east1)

           (<west>  ^spelling |west| 
                    ^structure-type CARDINAL
                    ^number singular
                    ^referent @west1)

           (<n>  ^spelling  |n|
                 ^structure-type CARDINAL
                 ^number singular
                 ^referent @north1)

           (<s>  ^spelling  |s|
