sp {top-state*apply*init-agent*create*world-state*mobile
   (state <s> ^superstate nil
              ^agent-params.domain magicbot
              ^operator.name init-agent)
-->
   (<s> ^world <w>)
   (<w> ^objects <objs>
        ^predicates <preds>
        ^robot <rob>)
   (<rob> ^arm <arm>
          ^handle rosie
          ^predicates.name rosie
          ^moving-status stopped)
   (<arm> ^state external)
}

