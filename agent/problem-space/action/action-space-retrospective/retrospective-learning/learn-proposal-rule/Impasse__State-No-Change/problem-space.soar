sp {learn-proposal-rule-substate*elaborate*problem-space
   (state <s> ^name learn-proposal-rule-substate)
-->
   (<s> ^default-problem-space no)
   
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^world.copy-type deep
         ^learning <learn>)
   (<learn> ^use chunking)
}

sp {learn-proposal-rule*interrupt
   (state <s> ^name learn-proposal-rule)
-->
   (interrupt)
}

