
sp {push-procedural-subgoal*propose*create-goal-graph
   (state <s> ^name push-procedural-subgoal
              ^task-concept-network <aid>)
   (<aid> -^goal-graph)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-goal-graph)
}

sp {push-procedural-subgoal*apply*create-goal-graph
   (state <s> ^name push-procedural-subgoal
              ^operator.name create-goal-graph
              ^task-concept-network <tcn>)
-->
   (<tcn> ^goal-graph <g>)
   (<g> ^first none)
   (<s> ^to-store <tcn>)
}
