sp {push-procedural-subgoal*elaborate*things-to-store
   (state <s> ^name push-procedural-subgoal
              ^to-store <lti>)
-->
   (<s> ^things-to-store true)
}

sp {push-procedural-subgoal*propose*store-goal
   (state <s> ^name push-procedural-subgoal
              ^things-to-store true
             -^stored-goal true)
-->
   (<s> ^operator <o> +)
   (<o> ^name store-goal)
}

sp {push-procedural-subgoal*apply*store-goal
   (state <s> ^name push-procedural-subgoal
              ^operator.name store-goal
              ^to-store <lti>
              ^smem.command <cmd>)
-->
   (<cmd> ^store <lti>)

   (<s> ^stored-goal true)
}

