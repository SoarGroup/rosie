sp {push-procedural-subgoal*propose*create-goal-node
   (state <s> ^name push-procedural-subgoal
              ^subtask-handle <sub-handle>
              ^task-concept-network.goal-graph
             -^subtask-goal-node)
-->
   (<s> ^operator <o> + =)
   (<o> ^name create-goal-node
        ^subtask-handle <sub-handle>)
}

sp {push-procedural-subgoal*apply*create-goal-node
   (state <s> ^name push-procedural-subgoal
              ^operator <o>)
   (<o> ^name create-goal-node
        ^subtask-handle <sub-handle>)
-->
   (<s> ^subtask-goal-node <node>)
   (<node> ^item-type task-goal
           ^handle (make-constant-symbol |taskgoal|)
           ^predicates <preds>)
   (<preds>  ^pred-count 1
             ^1 <pred1>)
   (<pred1> ^type subtask
            ^subtask-handle <sub-handle>)

   (<s> ^to-store <node> <preds> <pred1>)
}
