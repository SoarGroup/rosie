sp {push-procedural-subgoal*propose*report-success
   (state <s> ^name push-procedural-subgoal
              ^stored-goal true)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {push-procedural-subgoal*apply*report-success
   (state <s> ^name push-procedural-subgoal
              ^operator.name report-success
              ^subtask-goal-node.handle <h>
              ^superstate <ss>)
-->
   (<ss> ^created-procedural-subgoal <h>)
}

sp {push-procedural-subgoal*apply*report-success*change*current-goal
   (state <s> ^name push-procedural-subgoal
              ^operator.name report-success
              ^subtask-goal-node.handle <h>
              ^task-operator <task-op>)
   (<task-op> ^current-goal { <old-goal> <> <h> })
-->
   (<task-op> ^current-goal <old-goal> -
              ^current-goal <h>)
}
