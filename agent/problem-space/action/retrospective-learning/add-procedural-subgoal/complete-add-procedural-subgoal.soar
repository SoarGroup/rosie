sp {add-procedural-subgoal*propose*complete-add-procedural-subgoal
   (state <s> ^name add-procedural-subgoal
              ^stored-goals true)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-add-procedural-subgoal)
}

sp {add-procedural-subgoal*apply*complete-add-procedural-subgoal
   (state <s> ^name add-procedural-subgoal
              ^operator.name complete-add-procedural-subgoal
              ^subtask-goal-node.handle <h>
              ^superstate <ss>)
-->
   (<ss> ^added-procedural-subgoal <h>)
}

