sp {op_go-to-waypoint*operator-tie*elaborate*problem-space*waypoint-search
   (state <s> ^superstate.name op_go-to-waypoint1
              ^impasse tie
              ^attribute operator)
-->
   (<s> ^default-problem-space no)

   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action-space waypoint-search
         ^execution-type internal
         ^world <world>
         ^learning <learn>
         ^perception <perc>)
   (<world> ^copy-type shallow)
   (<learn> ^type none)
   (<perc> ^attend-to-all-perception no)
}
