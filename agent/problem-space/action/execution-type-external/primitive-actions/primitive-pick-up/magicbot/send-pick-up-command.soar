sp {primitive-pick-up*magicbot*propose*send-pick-up-command
   (state <s> ^name primitive-pick-up
              ^agent-params.domain magicbot
              ^problem-space.execution-type external
              ^target-object <obj>
             -^current-task-segment.pick-up-command
             ^object-info.input-link-obj.tag-id <tag>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name send-pick-up-command
        ^perception-id <tag>)
}

sp {primitive-pick-up*magicbot*apply*send-pick-up-command
   (state <s> ^name primitive-pick-up
              ^agent-params.domain magicbot
              ^operator <o>
              ^io.output-link <ol>
              ^current-task-segment <seg>)
   (<o> ^name send-pick-up-command
        ^perception-id <perc-id>)
-->
   (<ol> ^do-control-law <cmd>)
   (<cmd> ^name pick-up
          ^parameters.object-id <perc-id>
          ^termination-condition <term>)
   (<term> ^name holding
           ^parameters.object-id <perc-id>)
   (<seg> ^pick-up-command <cmd>)
}
