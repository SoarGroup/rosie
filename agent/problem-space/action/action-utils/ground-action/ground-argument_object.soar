sp {ground-action*apply*ground-argument*object*abstract*1*predicate
  (state <s> ^name ground-action
             ^operator <o>)
  (<o> ^name ground-argument
       ^argument <arg>
       ^arg-type object
       ^match-type abstract
       ^object-rep <rep>)
  (<rep> ^<prop1> <pred1>)
  -{(<rep> ^{ <prop2> <> <prop1> })}
-->
  (<arg> ^instance <i>)
  (<i> ^arg-type object
       ^id <obj>)
  (<obj> ^item-type object
         ^predicates <preds>)
  (<preds> ^<prop1> <pred1>)
}

