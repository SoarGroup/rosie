              ^reference-action.<arg-name> <op-arg>)
  (<arg> ^generalized-rep <gen>
          -^instance)
  (<gen> ^arg-type object
         ^id <obj-slot>)
  (<parg> ^arg-type predicate
          ^2 <obj-slot>)
