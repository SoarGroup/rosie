sp {add-subtask-to-tcn*propose*create-subtask-structure
   (state <s> ^name add-subtask-to-tcn
              ^generalize-task-operator.general-task-rep <gen-task>
             -^subtask-structure)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-subtask-structure
        ^general-subtask-rep <gen-task>)
}

sp {add-subtask-to-tcn*apply*create-subtask-structure*create*subtask*handle
   (state <s> ^name add-subtask-to-tcn
              ^operator <o>)
   (<o> ^name create-subtask-structure
        ^general-subtask-rep <gen-sub>)
-->
   (<gen-sub> ^handle (make-constant-symbol |subtask|))
}

sp {add-subtask-to-tcn*apply*create-subtask-structure*mark*learned*no
   (state <s> ^name add-subtask-to-tcn
              ^operator <o>)
   (<o> ^name create-subtask-structure
        ^general-subtask-rep <gen-sub>)
-->
   (<gen-sub> ^learned no)
}

sp {add-subtask-to-tcn*apply*create-subtask-structure*copy*general-subtask-rep*as*subtask-structure
   (state <s> ^name add-subtask-to-tcn
              ^operator <o>)
   (<o> ^name create-subtask-structure
        ^general-subtask-rep <gen-sub>)
-->
   (<s> ^subtask-structure <gen-sub>)
}
