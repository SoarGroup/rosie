# look up the goal
sp {learn-select-next-goal-rule*elaborate*smem-query*goal-id
   (state <s> ^name learn-select-next-goal-rule
              ^goal-handle <goal-h>)
-->
   (<s> ^smem-query <query>)
   (<query> ^cue <cue>
            ^destination <s>
            ^result-name goal-id-copy
            ^depth 5)
   (<cue> ^handle <goal-h>)
}
