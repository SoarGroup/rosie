sp {learn-select-next-goal-rule*propose*ignore-goal*unmet*conditions
   (state <s> ^name learn-select-next-goal-rule
              ^predicate-test <pt>
             -^satisfied <pt>)
-->
   (<s> ^operator <o> + = <)
   (<o> ^name ignore-goal)
}

sp {learn-select-next-goal-rule*apply*ignore-goal*mark*superstate*ignore
   (state <s> ^name learn-select-next-goal-rule
              ^operator.name ignore-goal
              ^next-goal-link <next>
              ^superstate <ss>)
-->
   (<ss> ^ignore-goal <next>)
}
