sp {task-state-no-change*propose*learn-desired-elaboration-rule
   (state <s> ^name task-state-no-change
              ^current-goal <goal-h>
              ^goal-id <goal-id>
             -^desired)
-->
   (<s> ^operator <o> + =)
   (<o> ^name learn-desired-elaboration-rule
        ^goal-handle <goal-h>
        ^goal-id <goal-id>)
}
