### INSTANTIATE MODIFIER

sp {task-utils*instantiate-task-operator*propose*instantiate-task-argument*modifier*matches*default
   (state <s> ^instantiate-task-argument <arg>)
   (<arg> ^arg-type modifier
          ^general-arg-rep.id.default.predicate-handle <mod-handle>
         -^instantiation)
-->
  (<s> ^operator <o> + =)
  (<o> ^name instantiate-task-argument
       ^argument <arg>
       ^arg-type modifier
       ^match-type default
       ^handle <mod-handle>)
}

sp {task-utils*instantiate-task-operator*apply*instantiate-task-argument*modifier*create*instantiation
  (state <s> ^operator <o>)
  (<o> ^name instantiate-task-argument
       ^argument <arg>
       ^arg-type modifier
       ^handle <mod-handle>)
-->
  (<arg> ^instantiation <i>)
  (<i> ^handle <mod-handle>)
}

