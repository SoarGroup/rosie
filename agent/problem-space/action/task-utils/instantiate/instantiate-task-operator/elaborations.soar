sp {task-utils*instantiate-task-operator*elaborate*operator*info
   (state <s> ^name instantiate-task-operator
              ^superstate.operator <ssop>)
   (<ssop> ^name instantiate-task-operator
           ^destination <ito>
           ^general-task-rep <rep>     
           ^reference-task-op <task-op>   
           ^reference-tcn-proc <tcn>
           ^world <w>)
-->
   (<s> ^result-destination <ito>
        ^general-task-rep <rep>     
        ^reference-task-op <task-op>   
        ^reference-tcn-proc <tcn>
        ^world <w>)
}

sp {task-utils*instantiate-task-operator*elaborate*operator*info*no*references
   (state <s> ^name instantiate-task-operator
              ^superstate.operator <ssop>)
   (<ssop> ^name instantiate-task-operator
           ^destination <ito>
           ^general-task-rep <rep>     
          -^reference-task-op <task-op>   
          -^reference-tcn-proc <tcn>
           ^world <w>)
-->
   (<s> ^result-destination <ito>
        ^general-task-rep <rep>     
        ^world <w>)
}
