sp {task-utils*instantiate-task-operator*propose*complete-instantiate-task-operator
   (state <s> ^name instantiate-task-operator)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-instantiate-task-operator)
}

sp {task-utils*instantiate-task-operator*elaborate*complete-instantiate-task-operator*perform*from*general-task-rep
   (state <s> ^name instantiate-task-operator
              ^operator <o> +
              ^general-task-rep.perform <perf>)
   (<o> ^name complete-instantiate-task-operator)
-->
   (<o> ^perform <perf>)
}

sp {task-utils*instantiate-task-operator*elaborate*complete-instantiate-task-operator*perform*default
   (state <s> ^name instantiate-task-operator
              ^operator <o> +
              ^general-task-rep <task-rep>)
   (<o> ^name complete-instantiate-task-operator)
   (<task-rep> -^perform)
-->
   (<o> ^perform multiple)
}


sp {task-utils*instantiate-task-operator*apply*complete-instantiate-task-operator*create*task-operator-instantiation
   (state <s> ^name instantiate-task-operator
              ^operator <o>
              ^general-task-rep <rep>)
   (<o> ^name complete-instantiate-task-operator
        ^perform <perf>)
   (<rep> ^handle <handle>
          ^op_name <name>
          ^task-handle <th>)
-->
   (<s> ^task-operator-instantiation <task-op>)
   (<task-op> ^name <name>
              ^item-type task-operator
              ^task-handle <th>
              ^perform <perf>)
}

