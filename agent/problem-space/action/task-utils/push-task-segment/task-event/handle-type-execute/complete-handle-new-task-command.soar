
sp {handle-type-execute*propose*complete-handle-new-task-command
   (state <s> ^name handle-type-execute
              ^task-operator <task-op>)
-->
   (<s> ^operator <o> + <)
   (<o> ^name complete-handle-new-task-command)
}

sp {handle-type-execute*apply*complete-handle-new-task-command*push-task-segment
   (state <s> ^name handle-type-execute
              ^operator.name complete-handle-new-task-command
              ^task-operator <task-op>
              ^top-state.task-stack <stack>)
-->
   (<stack> ^push-task-operator <task-op>)
   (<task-op> ^task-source instruction)
}

sp {handle-type-execute*apply*complete-handle-new-task-command*update*interaction*segment*params
   (state <s> ^name handle-type-execute
              ^operator.name complete-handle-new-task-command
              ^command-message <msg>
              ^task-operator <task-op>
              ^top-state.interaction.stack.segment.purpose.parameters <params>)
-->
   (<params> ^task-operator <task-op>)
}
