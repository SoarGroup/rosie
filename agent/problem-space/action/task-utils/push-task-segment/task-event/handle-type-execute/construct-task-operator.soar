sp {handle-type-execute*propose*construct-task-operator
   (state <s> ^name handle-type-execute
              ^command-message <msg>
              ^task-concept-network <tcn>
             -^task-operator)
-->
   (<s> ^operator <o> +)
   (<o> ^name construct-task-operator
        ^command-message <msg>
        ^update-task-concept-network true)
}

sp {handle-type-execute*elaborate*construct-task-operator*conditions
   (state <s> ^name handle-type-execute
              ^operator <o> +
              ^conditions <conds>)
   (<o> ^name construct-task-operator
        ^command-message <msg>)
-->
   (<msg> ^conditions <conds>)
}


