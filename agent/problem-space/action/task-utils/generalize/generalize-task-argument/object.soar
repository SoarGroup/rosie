         ^instantiation <i>
        -^general-arg-rep)
  (<i> ^id <obj>)
-->
  (<s> ^operator <o> + =)
  (<o> ^name generalize-task-argument
       ^argument <arg>
       ^arg-type object
       ^match-type default
       ^instantiation <obj>
       ^predicates <preds>)
}

sp {task-utils*generalize-task-argument*elaborate*generalize-task-argument*object*default*predicates*sentence
  (state <s> ^task-concept-network.handle << say1 ask1 >>
