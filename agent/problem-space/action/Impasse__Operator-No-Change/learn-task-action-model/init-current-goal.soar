sp {learn-task-action-model*propose*init-current-goal
   (state <s> ^name learn-task-action-model
             -^task-operator.current-goal
              ^task-concept-network.goal-graph.handle <start-h>)
-->
   (<s> ^operator <o> + = >)
   (<o> ^name init-current-goal
        ^start-handle <start-h>)
}

sp {learn-task-action-model*apply*init-current-goal
   (state <s> ^name learn-task-action-model
              ^operator <o>
              ^task-operator <task-op>)
   (<o> ^name init-current-goal
        ^start-handle <start-h>)
-->
   (<task-op> ^current-goal.handle <start-h>)
}

