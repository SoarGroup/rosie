	      ^track-relation <tr>
	      -^action-status learning)
   (<d> ^achieve-state <asp>
	^achieve-state {<asp2> <> <asp>}
	^track-relation <dtr>)
   (<asp> ^p1 <obj>
   	  ^state <state>)
   (<asp2> ^p1 <obj2>
   	  ^state <state2>)
   (<dtr> ^p1 <val12>
   	  ^p2 <val22>
	  ^relation <prep>)
   (<tr> ^object1 <val32>
	 ^object2 <val42>
	 ^vpredicate <prep>
	 ^complete success)
   (<sobj> ^focus <f1>)
   (<obj> ^focus <f1>)
   (<sobj2> ^focus <f2>)
   (<obj2> ^focus <f2>)
   (<val12> ^focus <fval12>)
   (<val22> ^focus <fval22>)
   (<val32> ^focus <fval12>)
   (<val42> ^focus <fval22>)
-->
   (<s> ^success <d>)
}

