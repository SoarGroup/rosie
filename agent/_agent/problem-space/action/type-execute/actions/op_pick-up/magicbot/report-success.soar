sp {op_pick-up1*magicbot*propose*report-success
   (state <s> ^name op_pick-up1
              ^current-action.pick-up-command.status success)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {op_pick-up1*magicbot*apply*report-success*mark*finished
   (state <s> ^name op_pick-up1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}

sp {op_pick-up1*magicbot*apply*report-success*remove*in*predicate
   (state <s> ^name op_pick-up1
              ^operator.name report-success
              ^target-object <obj>
              ^top-state.world.predicates.predicate <in-pred>
              ^top-state.world.robot.current-location <loc>)
   (<in-pred> ^handle in1
              ^instance <i>)
   (<i> ^1 <obj> ^2 <loc>)
-->
   (<in-pred> ^instance <i> -)
}


sp {op_pick-up1*magicbot*apply*report-success*remove*holding*predicate
   (state <s> ^name op_pick-up1
              ^operator.name report-success
              ^target-object <obj>
              ^top-state.world.predicates.predicate <in-pred>)
   (<in-pred> ^handle holding1
              ^instance <i>)
   (<i> ^1 <obj> ^2 <person>)
-->
   (<in-pred> ^instance <i> -)
}


