sp {op_go-to*propose*report-success
   (state <s> ^name op_go-to1
              ^world.robot.current-waypoint <wp>
              ^desired-waypoint <wp>
              -^child-action)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {op_go-to*propose*report-success*drive-command*reached*object
   (state <s> ^name op_go-to1
              ^current-action.drive-command.status success)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {op_go-to*propose*report-success*drive-forward*stopped
   (state <s> ^name op_go-to1
              ^current-action.drive-forward-command.status success)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}


              
