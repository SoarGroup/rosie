sp {elaborate*state*sentence-1*store-current-sentence
    :o-support
   (state <s> ^name comprehension
              ^superstate <ss>
              ^top-state <ts>
              ^segment <seg>)
   (<ss> ^current-sentence-number <cs>)
   (<seg> ^original-sentence <first>)
   (<ts> ^game-scripting true)
-->
    (<ts> ^current-sentence <first>)
}

sp {apply*interpet-message*remove*current-sentence-number
   (state <s> ^top-state <ts>
              ^operator.name interpret-message)
   (<ts> ^game-scripting true ^current-sentence-number <csn>)
-->
   (<ts> ^n-sentence-number (+ <csn> 1))
   (<ts> ^current-sentence-number <csn> -)
}

sp {apply*wait-for-response*add*next-sentence-number
   (state <s> ^top-state <ts>
              ^operator.name wait-for-response)
   (<ts> ^game-scripting true ^n-sentence-number <csn> ^current-sentence <cs>)# -^stored-sentence)
-->
   (<ts> ^n-sentence-number <csn> -)
   (<ts> ^current-sentence <cs> -)
   (<ts> ^current-sentence-number <csn>)
}

