sp {op_go-to-location1*propose*go-to-waypoint
   (state <s> ^name op_go-to-location1
              ^desired-waypoint <wp>)
-->
   (<s> ^operator <o> + )
   (<o> ^name go-to-waypoint
        ^waypoint <wp>
        ^action-type composite)
}

sp {op_go-to-location1*elaborate*go-to-waypoint*current-waypoint
   (state <s> ^name op_go-to-location1
              ^operator <o> +
              ^superstate.world.robot.current-waypoint <wp>)
   (<o> ^name go-to-waypoint)
-->
   (<o> ^current-waypoint <wp>)
}
