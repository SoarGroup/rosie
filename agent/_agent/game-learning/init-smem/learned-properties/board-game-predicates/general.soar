#^type property 
smem --add {
#adjacent = next-to but not diagonal
   (@R1018 ^item-type predicate ^predicate-structure <ps> ^primary-rtype single ^argnum 2 ^property @G2000)# ^handle adjacent
   (<ps> ^conditions <cs3> <cs2>)
   
   (<cs3> ^result <r4> ^name diagonal-with1 ^parameter <param3> ^rtype single ^negative true ^args <ag3> ^type state-pair ^attribute prepositions)
   (<r4> ^set <res7>)
   (<ag3> ^1 <c5> ^2 <c6> ^num 2)
   (<param3> ^1 <p311> ^2 <p312> ^num 2)
   (<p311> ^param-id 1 ^type single)
   (<p312> ^param-id 2 ^type single)
   
   (<cs2> ^result <r8> ^name near1 ^parameter <param8> ^rtype single ^negative false ^args <ag8> ^type state-pair ^attribute prepositions)
   (<r8> ^set <res8>)
   (<ag8> ^1 <c5> ^2 <c6> ^num 2)
   (<param8> ^1 <p388> ^2 <p382> ^num 2)
   (<p388> ^param-id 1 ^type single)
   (<p382> ^param-id 2 ^type single)
  
   
    (<c5> ^args <A57> ^attribute input-arg ^name object ^negative false ^result <r5>
                 ^parameter <P225> ^rtype single)
    (<P225> ^1 <P224> ^num 1)
    (<r5> ^set <re0>)
    (<P224> ^param-id 1 ^type single)
    
   (<c6> ^args <A67> ^attribute input-arg ^name object ^negative false ^result <r6a>
                 ^parameter <P628> ^rtype single)
   (<r6a> ^set <re06>)
   (<P628> ^1 <P629> ^num 1)
   (<P629> ^param-id 2 ^type single)  
}