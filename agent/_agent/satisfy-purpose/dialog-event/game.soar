#LEARN CONDITION
sp {parameter-condition-message*propose*learn-condition
   (state <s> ^name dialog-event
              ^purpose <p>)
   (<p> ^type parameter-condition-message
        ^parameters <param>)
   (<param> ^parameter <type> ^attribute <cond>)
-->
   (<s> ^operator <o> +)
   (<o> ^name learn-condition
        ^category svs
        ^prep none
        ^negative false
        ^attribute <cond>
	    ^obja <obja>)
   (<obja> ^word object
          ^specifier DEF
          ^word <type>)
}
