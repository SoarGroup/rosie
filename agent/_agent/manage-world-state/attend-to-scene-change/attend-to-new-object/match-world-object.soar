sp {attend-to-new-object*propose*match-world-object*perception-obj
   (state <s> ^name attend-to-new-object
              ^source-type perception-obj
              ^source-obj.id <handle>
              ^input-link-obj.classifications <preds>
              ^source-obj <pobj>
             -^world-match-result <res>)
-->
   (<s> ^operator <o> +)
   (<o> ^name match-world-object
        ^object-handle <handle>
        ^object-predicates <preds>
        ^svs-obj <pobj>
        ^match-visible false)
}

sp {attend-to-new-object*propose*match-world-object*epmem-obj
   (state <s> ^name attend-to-new-object
              ^source-type epmem-obj
              ^source-obj.handle <handle>
              ^source-obj.predicates <preds>
             -^world-match-result <res>)
-->
   (<s> ^operator <o> +)
   (<o> ^name match-world-object
        ^object-handle <handle>
        ^object-predicates <preds>
        ^match-visible true)
}
