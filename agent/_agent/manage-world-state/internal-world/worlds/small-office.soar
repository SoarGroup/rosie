sp {top-state*domain*internal*elaborate*world*info*small-office
   (state <s> ^superstate nil
              ^world <w>)
   (<w> ^objects <objs>
        ^predicates <preds>)
-->
   (<objs> ^object <table> <red-block> <blue-block> <green-sphere>)

   (<table> ^handle table
            ^item-type object
            ^predicates <preds>)
   (<preds> ^category location ^name table
            ^arm-status not-grabbed ^visible true)
   
   (<red-block> ^handle red-block
                ^item-type object
                ^predicates <red-bl-preds>)
   (<red-bl-preds> ^category block ^color red1 ^shape box1
                   ^arm-status grabbed ^visible true)
   
   (<blue-block> ^handle blue-block
                 ^item-type object
                 ^predicates <blue-bl-preds>)
   (<blue-bl-preds> ^category block ^color blue1 ^shape box1
                    ^arm-status not-grabbed ^visible true)
   
   (<green-sphere> ^handle green-sphere
                   ^item-type object
                   ^predicates <green-sp-preds>)
   (<green-sp-preds> ^category block ^color green1 ^shape sphere1
                     ^arm-status not-grabbed ^visible true)
}

sp {op_move*propose*nothing
   (state <s> ^name op_move1
              ^problem-space.action-space retrospective
             -^desired.satisfied true)
-->
   (<s> ^operator <o> +)
   (<o> ^name nothing
        ^action-handle nothing1)
}