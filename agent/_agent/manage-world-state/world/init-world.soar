sp {top-state*elaborate*simulated*mobile*objects
   (state <s> ^superstate nil
              ^agent-params.domain mobile
              ^world-usage external
              ^world <world>)
   (<world> ^objects <objs>
            ^predicates.predicate <in-pred>)
   (<in-pred> ^handle in1)
-->
  (<objs> ^object <conference> <soar> <kitchen> <main> <bob>)

   	(<conference> ^handle conference-room1 ^item-type object ^predicates <confp> ^english-description |conference room| ^waypoint wp08)
  	(<confp> ^category location ^name conference1 ^spatial-shape room1 ^visibility not-visible1)

	(<soar> ^handle soar-office1 ^item-type object ^predicates <soarp> ^english-description |soar office| ^waypoint wp07)
  	(<soarp> ^category location ^name soar1 ^spatial-shape room1 ^visibility not-visible1)

	(<kitchen> ^handle kitchen-room1 ^item-type object ^predicates <kitchenp> ^english-description |kitchen| ^waypoint wp02)
  	(<kitchenp> ^category location ^name kitchen1 ^spatial-shape room1 ^visibility not-visible1)

   	(<main> ^handle main-office1 ^item-type object ^predicates <mainp> ^english-description |main office| ^waypoint wp06)
     	(<mainp> ^category location ^name main1 ^spatial-shape room1 ^visibility not-visible1)

	(<bob> ^handle bob-office1 ^item-type object ^predicates <bobp> ^english-description |Bob's office| ^waypoint wp01)
  	(<bobp> ^category location ^name bobs1 ^spatial-shape room1 ^visibility not-visible1)
}

sp {top-state*propose*remove-soda
   (state <s> ^superstate nil
              ^world.objects.object.handle soda1)
-->
   (<s> ^operator <o> + > !)
   (<o> ^name remove-soda)
}

sp {top-state*apply*remove-soda
   (state <s> ^superstate nil
              ^operator.name remove-soda
              ^world.objects <objs>
              ^world.predicates.predicate <pred>)
   (<objs> ^object <soda>)
   (<soda> ^handle << soda1 red-package1 >>)
   (<pred> ^handle in1
           ^instance <i>)
   (<i> ^1 <soda>)
-->
   (<objs> ^object <soda> -)
   (<pred> ^instance <i> -)
}

