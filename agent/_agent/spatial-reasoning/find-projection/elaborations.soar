sp {find-projection*elaborate*copy-down-attrs
   (state <s> ^name find-projection
              ^superstate.operator.a.id <ida>
              ^superstate.operator.b.id <idb>
              ^superstate.operator.type <prep>)
-->
   (<s> ^result <res>
        ^a <ida>
        ^b <idb>
        ^prep <prep>)
}

sp {find-projection*elaborate*copy-down-attrs*svs-link
   (state <s> ^name find-projection
              ^superstate.operator <o>)
   (<o> ^svs-link <svs>)
-->
   (<s> ^svs-link <svs>)
}
sp {find-projection*elaborate*copy-down-attrs*svs-no-link-usetopstate
   (state <s> ^name find-projection
              ^topstate.svs <svs>
              ^superstate.operator <o>)
   (<o> -^svs-link )
-->
   (<s> ^svs-link <svs>)
}



