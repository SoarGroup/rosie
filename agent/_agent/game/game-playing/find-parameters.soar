   (<rels> ^with <id> ^preposition <name>)}
-->
   (<s> ^operator <o> + =)
   (<o> ^name find-relations
        ^category svs
#        ^svs-link <svs>
        ^obj <id>
        ^ind 1
        ^found-prep <fprep>
        ^prep <name>)
}

sp {find-parameters*failure*propose*find-relations*smaller-than
   (state <s> ^name find-parameters
