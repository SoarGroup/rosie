# Object affordances and properties
smem --add {
   # is-visible1: visible1 or not-visible1
   #    whether the robot can currently see the object
   (@100130 ^handle is-visible1 ^type visual ^item-type property)
   (@100131 ^handle visible1 ^property @100130)
   (@100132 ^handle not-visible1 ^property @100130)
   # AM: I don't know if unknown-visible is needed
   (@100133 ^handle unknown-visible1 ^property @100130)

   # is-confirmed1: confirmed1 or not-confirmed1
   #    whether the object has a known position in the world
   (@100134 ^handle is-confirmed1 ^type visual ^item-type property)
   (@100135 ^handle confirmed1 ^property @100134)
   (@100136 ^handle not-confirmed1 ^property @100134)

   # is-reachable1: reachable1 or not-reachable1
   #    whether the object/person is within arm's reach (can interact with)
   (@100137 ^handle is-reachable1 ^type visual ^item-type property)
   (@100138 ^handle reachable1 ^property @100137)
   (@100139 ^handle not-reachable1 ^property @100137)

   # is-grabbed1: grabbed1 or not-grabbed1
   #    whether the robot is holding the object
   (@100140 ^handle grabbable1 ^property @100128 ^default @100141)
   (@100142 ^handle is-grabbed1 ^type visual ^item-type property)
   (@100143 ^handle grabbed1 ^property @100142)
   (@100141 ^handle not-grabbed1 ^property @100142)

   # is-activated1: activated1 or not-activated1
   #    whether the robot is holding the object
   (@100144 ^handle activatable1 ^property @100128 ^default @100145)
   (@100146 ^handle is-activated1 ^type visual ^item-type property)
   (@100147 ^handle activated1 ^property @100146)
   (@100145 ^handle not-activated1 ^property @100146)

   # is-open1: open1 or not-open1
   #    whether the robot is holding the object
   (@100148 ^handle openable1 ^property @100128 ^default @100149)
   (@100150 ^handle is-open1 ^type visual ^item-type property)
   (@100149 ^handle open2 ^property @100150)
   (@100151 ^handle not-open1 ^property @100150)

   (@100152 ^handle always-open1 ^property @100128)

   # is-locked1: locked1 or not-locked1
   #    whether the object is currently locked (given by perception)
   (@100153 ^handle lockable1 ^property @100128 ^default @100154)
   (@100155 ^handle is-locked1 ^type visual ^item-type property)
   (@100156 ^handle locked1 ^property @100155)
   (@100154 ^handle not-locked1 ^property @100155)

   # connected1 or not-connected1
   (@100157 ^handle connected1 ^property @100158)
   (@100159 ^handle not-connected1 ^property @100158)

#   # following1 or not-following1
#   (@following1 ^handle following1 ^property @is-following1)
#   (@not-following1 ^handle not-following1 ^property @is-following1)

   # is-filled1: filled1 or empty1
   (@100160 ^handle fillable1 ^property @100128 ^default @100161)

   (@100162 ^handle contents1 ^item-type property)
   (@100161 ^handle empty1 ^property @100162)

   # drain1: You can empty a fillable object with it
   (@100163 ^handle drain1 ^property @100128)

   # dispenser1: Something that you can pour into something else
   (@100164 ^handle dispenser1 ^property @100128)

   # Other affordances
   (@100165 ^handle pourable1 ^property @100128)
   (@100166 ^handle pressable1 ^property @100128)

   (@100167 ^handle surface1 ^property @100128)
   (@100168 ^handle receptacle1 ^property @100128)

   (@100144 ^handle activatable1 ^property @100128)

}
