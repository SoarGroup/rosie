smem --add {
           (<more> ^spelling |more|
                   ^assigners <pp1> <pp2> <pp3>
                   ^structure-type C-ADJ  # Comparative Adjective
                   ^converts-to C-ADJ-P
                   ^semantic-structure relation
                   ^referent @100434) 
           (<pp1> ^structure-type N
                   ^before <pp2> <pp3>
                   ^relative-position after
                   ^semantic-structure arg1
                   ^syntactic-structure head
                   ^required true)    
           (<pp2> ^structure-type THAN
                  ^before <pp3>
                  ^required true
                  ^syntactic-structure nil)
           (<pp3> ^structure-type N
                  ^relative-position after
                  ^semantic-structure arg2
                  ^syntactic-structure complement
                  ^required true)
           
}

smem --add {
           (<more> ^spelling |fewer|
                   ^assigners <pp1> <pp2> <pp3>
                   ^structure-type C-ADJ  # Comparative Adjective
                   ^converts-to C-ADJ-P
                   ^semantic-structure relation
                   ^referent @100436) 
           (<pp1> ^structure-type N
                   ^before <pp2> <pp3>
                   ^relative-position after
                   ^semantic-structure arg1
                   ^syntactic-structure head
                   ^required true)    
           (<pp2> ^structure-type THAN
                  ^before <pp3>
                  ^required true
                  ^syntactic-structure nil)
           (<pp3> ^structure-type N
                  ^relative-position after
                  ^semantic-structure arg2
                  ^syntactic-structure complement
                  ^required true)
           (<than> ^spelling |than|
                  ^structure-type THAN)
           
}

